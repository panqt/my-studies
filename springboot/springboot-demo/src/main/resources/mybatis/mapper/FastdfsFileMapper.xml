<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pers.panqt.springboot.mybatis.FastdfsFileMapper">
    <insert id="insert" parameterType="java.lang.String">
        insert into fastfile values (#{fileid})
    </insert>
    
    <delete id="delete" parameterType="java.lang.String">
        delete from fastfile where file_id = #{fileId}
    </delete>

    <select id="findById" parameterType="java.lang.String" resultType="java.lang.String">
      select file_id from fastfile where file_id = #{fileId}
    </select>
    
    <select id="findAll" resultType="pers.panqt.springboot.fastdfs.File">
        select file_id from fastfile where 1 = 1
    </select>
</mapper>